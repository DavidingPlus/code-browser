<dec f='boost-1.85.0/libs/url/include/boost/url/url_view_base.hpp' l='2268' type='boost::urls::pct_string_view boost::urls::url_view_base::encoded_origin() const'/>
<doc f='boost-1.85.0/libs/url/include/boost/url/url_view_base.hpp' l='2242'>/** Return the origin

        If an authority is present, this
        function returns the scheme and
        authority portion of the url.
        Otherwise, an empty string is
        returned.
        The returned string may contain
        percent escapes.

        @par Example
        @code
        assert( url_view( &quot;http://www.example.com:8080/index.htm?text=none#h1&quot; ).encoded_origin() == &quot;http://www.example.com:8080&quot; );
        @endcode

        @par Complexity
        Constant.

        @par Exception Safety
        Throws nothing.

        @see
            @ref encoded_resource,
            @ref encoded_target.
    */</doc>
<def f='boost-1.85.0/libs/url/src/url_view_base.cpp' l='608' ll='615' type='boost::urls::pct_string_view boost::urls::url_view_base::encoded_origin() const'/>
