<def f='boost-1.85.0/libs/container/include/boost/container/pmr/polymorphic_allocator.hpp' l='90' ll='91' type='void boost::container::pmr::polymorphic_allocator::deallocate(T * p, size_t n)'/>
<doc f='boost-1.85.0/libs/container/include/boost/container/pmr/polymorphic_allocator.hpp' l='84'>//! &lt;b&gt;Requires&lt;/b&gt;: p was allocated from a memory resource, x, equal to *m_resource,
   //! using `x.allocate(n * sizeof(T), alignof(T))`.
   //!
   //! &lt;b&gt;Effects&lt;/b&gt;: Equivalent to m_resource-&gt;deallocate(p, n * sizeof(T), alignof(T)).
   //!
   //! &lt;b&gt;Throws&lt;/b&gt;: Nothing.</doc>
