<dec f='qtbase-6.5.0/src/corelib/tools/qcommandlineoption.h' l='31' type='void QCommandLineOption::QCommandLineOption(const QStringList &amp; names, const QString &amp; description, const QString &amp; valueName = QString(), const QString &amp; defaultValue = QString())'/>
