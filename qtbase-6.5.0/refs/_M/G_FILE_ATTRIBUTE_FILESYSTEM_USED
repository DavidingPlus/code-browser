<doc f='include/glib-2.0/gio/gfileinfo.h' l='802'>/**
 * G_FILE_ATTRIBUTE_FILESYSTEM_USED:
 *
 * A key in the &quot;filesystem&quot; namespace for getting the number of bytes of used on the
 * file system. Corresponding #GFileAttributeType is
 * %G_FILE_ATTRIBUTE_TYPE_UINT64.
 *
 * Since: 2.32
