<mbr r='iw_scan_req::bssid' o='32' t='struct sockaddr'/>
<mbr r='iw_scan_req::essid' o='160' t='__u8[32]'/>
<mbr r='iw_scan_req::min_channel_time' o='416' t='__u32'/>
<mbr r='iw_scan_req::max_channel_time' o='448' t='__u32'/>
<mbr r='iw_scan_req::channel_list' o='480' t='struct iw_freq[32]'/>
