<mbr r='_GDBusMethodInfo::in_args' o='128' t='GDBusArgInfo **'/>
<mbr r='_GDBusMethodInfo::out_args' o='192' t='GDBusArgInfo **'/>
<mbr r='_GDBusMethodInfo::annotations' o='256' t='GDBusAnnotationInfo **'/>
