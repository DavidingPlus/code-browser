<mbr r='_GDBusNodeInfo::interfaces' o='128' t='GDBusInterfaceInfo **'/>
<mbr r='_GDBusNodeInfo::nodes' o='192' t='GDBusNodeInfo **'/>
<mbr r='_GDBusNodeInfo::annotations' o='256' t='GDBusAnnotationInfo **'/>
